<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE application SYSTEM "dtds/application.dtd"><application name="SCIS"> 		<!-- 	A default module for each entity is assumed with the	controllers on <default-module/>	-->	<default-module>		  		<controller name="TypicalScis"/>	</default-module>		<!-- 	You can define additional modules, for example:	  	<module name="OnlyReadTeacher">		<model name="Teacher"/>		<controller name="Print"/>	</module>			-->		<!-- Some Read-Only Modules -->	<module name="Revision">		<env-var name="XAVA_SEARCH_ACTION" value="CRUD.searchReadOnly"/>		<model name="Revision"/>		<controller name="ReadOnlyScis"/> 	</module>	<module name="RevisionChanges">		<env-var name="XAVA_SEARCH_ACTION" value="CRUD.searchReadOnly"/>		<model name="RevisionChanges"/>		<controller name="ReadOnlyScis"/> 	</module>		<module name="Podium">		<web-view url="custom-jsp/podium.jsp"/>		<model name="SpeleoObject"/><!-- needed? -->		<mode-controller name="Void"/>	</module>	</application>